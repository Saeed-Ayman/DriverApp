{
    "builds": [
        {
            "src": "/public/**",
            "use": "@vercel/static"
        }
    ],
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
