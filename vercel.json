{
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.7.1"
        },
        "serve-static": {
            "runtime": "nodejs@latest",
            "handler": "serveStatic.js"
        }
    },
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
