{
    "builds": [
        { "src": "/api/index.php", "use": "vercel-php@0.7.1" },
        { "src": "/public/**", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/storage/(.*)",
            "dest": "public/storage/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ]
}
